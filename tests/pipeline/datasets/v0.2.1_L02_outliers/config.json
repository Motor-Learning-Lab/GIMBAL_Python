{
  "meta": {
    "name": "L02_outliers",
    "version": "0.2.1",
    "description": "Outlier stress test: 10% outliers with wide Gaussian distribution",
    "seed": 44,
    "dt": 0.0167,
    "T": 1800
  },
  
  "dataset_spec": {
    "skeleton": {
      "joint_names": ["root", "proximal", "distal_left", "distal_right"],
      "parents": [-1, 0, 1, 1],
      "lengths": [0, 10, 8, 8]
    },
    
    "states": {
      "num_states": 1,
      "transition_matrix": [[1.0]]
    },
    
    "motion": {
      "per_state_params": {
        "0": {
          "mu": [[0, 0, 1], [0.7, 0, 0.7], [-0.7, 0, 0.7]],
          "omega": 3.0,
          "zeta": 0.7,
          "sigma_pose": 0.1
        }
      },
      "root_params": {
        "per_state": {
          "0": {
            "mu": [0, 0, 100]
          }
        },
        "omega": 2.0,
        "zeta": 0.8,
        "sigma_pose": 5.0,
        "init_pos": [0, 0, 100]
      }
    },
    
    "cameras": {
      "image_size": [1280, 720],
      "cameras": [
        {
          "name": "cam0_front",
          "K": [[640, 0, 640], [0, 640, 360], [0, 0, 1]],
          "R": [[1, 0, 0], [0, 0.866, -0.5], [0, 0.5, 0.866]],
          "t": [-50, 0, -50]
        },
        {
          "name": "cam1_side",
          "K": [[640, 0, 640], [0, 640, 360], [0, 0, 1]],
          "R": [[0, 0, 1], [0, 1, 0], [-1, 0, 0]],
          "t": [0, -50, -50]
        },
        {
          "name": "cam2_overhead",
          "K": [[640, 0, 640], [0, 640, 360], [0, 0, 1]],
          "R": [[1, 0, 0], [0, 1, 0], [0, 0, 1]],
          "t": [0, 0, -180]
        }
      ]
    },
    
    "observation": {
      "noise_px": 2.0,
      "outlier_enabled": true,
      "outlier_fraction": 0.1,
      "outlier_std_px": 50.0,
      "missingness_enabled": false,
      "missingness_bernoulli_p": 0.0
    }
  },
  
  "output": {
    "run_name": "L02_outliers_v0.2.1"
  }
}
